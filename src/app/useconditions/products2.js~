angular.module( 'ngBoilerplate.products', [
  'ui.router',
  'placeholders',
  'ui.bootstrap'
])

.config(function config( $stateProvider ) {
  $stateProvider.state( 'products2', {
    url: '/products2',
    views: {
      "main": {
        controller: 'ProductsCtrl',
        templateUrl: 'products2/products2.tpl.html'
      }
    },
    data:{ pageTitle: 'Nos produits 2' }
  });
})

.controller( 'ProductsCtrl', function ProductsCtrl( $scope ) {
})

;
